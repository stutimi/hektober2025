class Solution {
public:
    vector<vector<int>> mergeOverlap(vector<vector<int>>& a) {
        if (a.empty()) return {};
        sort(a.begin(), a.end());
        vector<vector<int>> res = {a[0]};
        for (int i = 1; i < a.size(); i++) 
            if (a[i][0] <= res.back()[1]) 
                res.back()[1] = max(res.back()[1], a[i][1]);
            else 
                res.push_back(a[i]);
        return res;
    }
};
